{
    "name": "notion-bookshelf-importer/notion-bookshelf-importer",
    "description": "Laravel webhook backend that extracts book metadata from Amazon product pages and updates Notion.",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "laravel/pint": "^1.26",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
